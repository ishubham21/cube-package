{
  "name": "cube-package",
  "version": "1.0.9",
  "description": "An NPM package to render markdown",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ishubham21/cube-package.git"
  },
  "author": "Shubham Gautam <https://meshubham.live>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "rm -rf ./dist && tsc",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "lint": "eslint \"**/*.{js,ts}\" --quiet",
    "lint:fix": "eslint \"**/*.{js,ts}\" --fix",
    "push:gh": "yarn run build && git add . && git commit -m \"adding a new build version\" && git push origin main",
    "prebuild": "rm -rf ./dist",
    "preversion": "yarn run build",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "babel-jest": "^28.1.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "registry": "https://registry-url"
  }
}
